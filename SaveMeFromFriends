if not game:IsLoaded() then 
    repeat game.Loaded:Wait()
    until game:IsLoaded()
end
game.StarterGui:SetCore("SendNotification",{
    Icon = "http://www.roblox.com/asset/?id=";
    Title = "System";
    Text = "GameRunning!";
    Duration = 5;
})
local p = game.Players.LocalPlayer
local Char = p.Character or p.CharacterAdded:Wait() 
local rootpart = Char:FindFirstChild("HumanoidRootPart")
local hum = Char:FindFirstChild("Humanoid")

local PartLeg = game:GetService("Workspace"):FindFirstChild("PartLeg")
    if PartLeg then
        PartLeg:Destroy()
    end
local Part = Instance.new("Part",workspace)
Part.Name = "PartLeg"
Part.CanCollide = true
Part.Anchored = true
Part.Transparency = 0
Part.Size = Vector3.new(15,0.2,15)
function Part1()
    game:GetService("Workspace"):FindFirstChild("PartLeg").CFrame = rootpart.CFrame * CFrame.new(0,-3,0)
end
local repo = 'https://raw.githubusercontent.com/THU9DEV/LinoriaLib/main/'

local Library = loadstring(game:HttpGet(repo .. 'Library.lua'))()
local ThemeManager = loadstring(game:HttpGet(repo .. 'addons/ThemeManager.lua'))()
local SaveManager = loadstring(game:HttpGet(repo .. 'addons/SaveManager.lua'))()

local Window = Library:CreateWindow({
    Title = 'Xser',
    Center = true,
    AutoShow = true,
    TabPadding = 8,
    MenuFadeTime = 0.2
})

local Tabs = {
    -- Creates a new tab titled Main
    Main = Window:AddTab('Teleport'),
    ['UI Settings'] = Window:AddTab('UI Settings'),
}
local LeftGroupBox = Tabs.Main:AddLeftGroupbox('Main')


local TabBox = Tabs.Main:AddLeftTabbox() 
Plr = {}
for i,v in pairs(game:GetService("Players"):GetChildren()) do
    table.insert(Plr,v.Name) 
end

LeftGroupBox:AddDropdown('MyDropdown', {
    Values = Plr,
    Default = 1,
    Multi = false, 

    Text = 'Select Player',
    Tooltip = 'Select Target', 

    Callback = function(t)
        PlayerTP = t
    end
})
local modeList = {
    [1] = "Bypass",
    [2] = "Tween",
}
LeftGroupBox:AddDropdown('MyDropdown', {
    Values = modeList,
    Default = 1,
    Multi = false, 

    Text = 'Select Mode',
    Tooltip = 'Select Mode', 

    Callback = function(t)
        modeTP = t
    end
})

LeftGroupBox:AddToggle('MyToggle', {
    Text = 'Teleport',
    Default = false, -- Default value (true / false)
    Tooltip = 'Click to Teleport',

    Callback = function(t)
        _G.Loop = t
        local target = game.Players[PlayerTP]
    local targetChar = target.Character or target.CharacterAdded:Wait()
    local targetrootpart = targetChar:FindFirstChild("HumanoidRootPart") 
    if modeTP == "Bypass" then
    pcall(function()
        while _G.Loop do task.wait()
            rootpart.CFrame = targetrootpart.CFrame * CFrame.new(0,7,0)
            Part1()
        end
    end)   
end
if modeTP == "Tween" then
    local TweenService  = game:GetService("TweenService")
local noclipE       = false
local antifall      = false

local function noclip()
    for i, v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v:IsA("BasePart") and v.CanCollide == true then
            v.CanCollide = false
            game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
        end
    end
end

local function moveto(obj, speed)
    local info = TweenInfo.new(((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - obj.Position).Magnitude)/ speed,Enum.EasingStyle.Linear)
    local tween = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, info, {CFrame = obj})

    if not game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
        antifall = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart)
        antifall.Velocity = Vector3.new(0,0,0)
        noclipE = game:GetService("RunService").Stepped:Connect(noclip)
        tween:Play()
    end
        
    tween.Completed:Connect(function()
        antifall:Destroy()
        noclipE:Disconnect()
    end)
end
while _G.Loop do task.wait()
    moveto(targetrootpart.CFrame * CFrame.new(0,7,0) + Vector3.new(0,0,0), 100)
game:GetService("VirtualUser"):ClickButton1(Vector2.new(9e9, 9e9))
Part1()
end
end
    end
})
