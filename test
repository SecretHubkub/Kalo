local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Title of the library", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Tp player"
})

local warp = game.Players.LocalPlayer.Character.HumanoidRootPart
Plr = {}
player = nil
for i,v in pairs(game:GetService("Players"):GetPlayers()) do 
    table.insert(Plr,v.Name)
end

Tab:AddDropdown({
	Name = "Select Player",
	Default = "1",
	Options = Plr,
	Callback = function(Value)
		player = Value
	end    
})

Tab:AddToggle({
	Name = "tp to player",
	Default = false,
	Callback = function(Value)
		_G.tp = Value
        while _G.tp do wait()

            warp.CFrame = game.Players[player].Character.HumanoidRootPart.CFrame
        end
	end    
})


local Tab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "Auto Farm"
})

Tab:AddToggle({
	Name = "Auto dumbel",
	Default = false,
	Callback = function(Value)
		_G.Autodb = Value
        while _G.Autodp do wait()
            local tool = "Weight"
            game.Players.LocalPlayer.Character.Humanoid:EquipTool(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(tool))
            local ohString1 = "rep"
            game:GetService("Players").LocalPlayer.muscleEvent:FireServer(ohString1)
        end
	end    
})
